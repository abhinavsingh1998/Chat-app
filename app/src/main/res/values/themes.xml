<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.HOABL" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/app_color</item>
        <item name="colorPrimaryVariant">@color/app_color</item>
        <item name="colorOnPrimary">@color/app_color</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/app_color</item>
        <item name="colorSecondaryVariant">@color/app_color</item>
        <item name="colorOnSecondary">@color/app_color</item>
        <!-- Status bar color. -->
        <!--
                <item name="android:fontFamily">@font/blooming_elegant_regular</item>
        -->
        <item name="android:statusBarColor" tools:targetApi="lollipop">@color/black</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">false</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <!-- Customize your theme here. -->
    </style>


    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:elevation" tools:targetApi="lollipop">0dp</item>
        <item name="elevation">0dp</item>
    </style>

    <style name="BottomNavigationView" parent="Widget.MaterialComponents.TextView">
        <item name="android:fontFamily">@font/jost_light</item>
        <item name="android:textSize">@dimen/text_size_10</item>
        <item name="android:textColor">@color/text_bluelightcolor</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="CustomTextInputLayout" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="boxCollapsedPaddingTop">0dp</item>
        <item name="boxBackgroundMode">none</item>
        <item name="android:paddingTop">0dp</item>
        <item name="boxBackgroundColor">@color/white</item>
        <item name="android:textColorHint">@color/text_grey_color</item>
        <item name="hintTextColor">@color/app_color</item>
        <item name="boxStrokeColor">@color/app_color</item>
    </style>

    <style name="CustomTextInputLayoutWithSpinner" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense.ExposedDropdownMenu">
        <item name="boxCollapsedPaddingTop">0dp</item>
        <item name="boxBackgroundMode">outline</item>
        <item name="android:paddingTop">0dp</item>
        <item name="boxBackgroundColor">@color/white</item>
        <item name="android:textColorHint">@color/text_grey_color</item>
        <item name="hintTextColor">@color/app_color</item>
        <item name="boxStrokeColor">@color/app_color</item>
        <item name="font">@font/jost_medium</item>

    </style>

    <style name="tab_text" parent="@android:style/TextAppearance.Widget.TabWidget">
        <item name="fontFamily">@font/jost_medium</item>
        <item name="android:fontFamily">@font/jost_medium</item>
    </style>


    <style name="CustomTextInputEditText">
        <item name="android:singleLine">true</item>
        <item name="android:minLines">1</item>
        <item name="android:maxLines">1</item>
        <item name="maxLines">1</item>
        <item name="paddingStart">@dimen/_8sdp</item>
        <item name="paddingEnd">@dimen/_2sdp</item>
        <item name="android:textColor">@color/text_color</item>
        <item name="android:paddingTop">@dimen/margin_size_12</item>
        <item name="android:paddingBottom">@dimen/margin_size_12</item>
    </style>

    <style name="CustomTextInputEditTextWithSpinner">
        <item name="android:singleLine">true</item>
        <item name="android:minLines">1</item>
        <item name="android:maxLines">1</item>
        <item name="maxLines">1</item>
        <item name="paddingStart">@dimen/_8sdp</item>
        <item name="paddingEnd">@dimen/_2sdp</item>
        <item name="android:textColor">@color/text_color</item>
        <item name="android:paddingTop">@dimen/margin_size_12</item>
